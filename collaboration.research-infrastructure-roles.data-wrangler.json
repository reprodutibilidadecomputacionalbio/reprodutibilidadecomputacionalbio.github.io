em","spread":true,"position":{"start":{"line":34,"column":1},"end":{"line":34,"column":1}},"children":[{"type":"text","value":"Sharing and reviewing resources and code","position":{"start":{"line":34,"column":1},"end":{"line":34,"column":1}},"key":"osuKeL7144"}],"key":"OvZKrxHrOi"},{"type":"listItem","spread":true,"position":{"start":{"line":35,"column":1},"end":{"line":35,"column":1}},"children":[{"type":"text","value":"Maintaining communication across multiple teams","position":{"start":{"line":35,"column":1},"end":{"line":35,"column":1}},"key":"vfM2CmMqgb"}],"key":"TE73GEJgt9"},{"type":"listItem","spread":true,"position":{"start":{"line":36,"column":1},"end":{"line":37,"column":1}},"children":[{"type":"text","value":"Running workshops on data wrangling and related skills","position":{"start":{"line":36,"column":1},"end":{"line":36,"column":1}},"key":"ohGHbuWXGX"}],"key":"FrpbkMjrcL"}],"key":"QL98nvXXKI"},{"type":"heading","depth":2,"position":{"start":{"line":39,"column":1},"end":{"line":39,"column":1}},"children":[{"type":"text","value":"What qualifications or skills do you need to be a Data Wrangler?","position":{"start":{"line":39,"column":1},"end":{"line":39,"column":1}},"key":"pHCPHWsqLa"}],"label":"cl-infrastructure-data-wranglers-skills","identifier":"cl-infrastructure-data-wranglers-skills","html_id":"cl-infrastructure-data-wranglers-skills","key":"BTQoXoRWSI"},{"type":"paragraph","position":{"start":{"line":41,"column":1},"end":{"line":46,"column":1}},"children":[{"type":"text","value":"Data Wranglers should have experience with programming (no specific language required, but there is a wider adoption of both ","position":{"start":{"line":41,"column":1},"end":{"line":41,"column":1}},"key":"aQeSoRA4Rk"},{"type":"inlineCode","value":"R","position":{"start":{"line":41,"column":1},"end":{"line":41,"column":1}},"key":"ROJ4wCWdwM"},{"type":"text","value":" and ","position":{"start":{"line":41,"column":1},"end":{"line":41,"column":1}},"key":"XkLf8kYHzL"},{"type":"inlineCode","value":"Python","position":{"start":{"line":41,"column":1},"end":{"line":41,"column":1}},"key":"wzTTCKSzV6"},{"type":"text","value":"), database querying (","position":{"start":{"line":41,"column":1},"end":{"line":41,"column":1}},"key":"FyX0EPlQq5"},{"type":"inlineCode","value":"SQL","position":{"start":{"line":41,"column":1},"end":{"line":41,"column":1}},"key":"jr0VCZp1Qj"},{"type":"text","value":") and data analysis.\nThey will have an educational background to equip them to engage with the specific research data objects relevant to the projects they will work on.\nTherefore, they will have undergraduate and postgraduate degrees, or equivalent experience.\nAs with many data science and research infrastructure roles, further relevant training and specialisation can happen on the job.\nThey need good problem solving skills, with a curiosity and willingness to learn.\nLastly, good interpersonal skills are required in order to work with people with many different backgrounds, skillsets and priorities.","position":{"start":{"line":41,"column":1},"end":{"line":41,"column":1}},"key":"nQkHVLY4pI"}],"key":"n8NDxpHWL3"},{"type":"heading","depth":2,"position":{"start":{"line":49,"column":1},"end":{"line":49,"column":1}},"children":[{"type":"text","value":"Challenges for Data Wranglers","position":{"start":{"line":49,"column":1},"end":{"line":49,"column":1}},"key":"ll0lM3RUA2"}],"label":"cl-infrastructure-data-wranglers-challenges","identifier":"cl-infrastructure-data-wranglers-challenges","html_id":"cl-infrastructure-data-wranglers-challenges","key":"nT0aR7pOFE"},{"type":"paragraph","position":{"start":{"line":51,"column":1},"end":{"line":51,"column":1}},"children":[{"type":"text","value":"Some key challenges of a Data Wrangler role are:","position":{"start":{"line":51,"column":1},"end":{"line":51,"column":1}},"key":"F9gqsY8P6J"}],"key":"NuMEu4ezcs"},{"type":"list","ordered":false,"spread":false,"position":{"start":{"line":53,"column":1},"end":{"line":59,"column":1}},"children":[{"type":"listItem","spread":true,"position":{"start":{"line":53,"column":1},"end":{"line":53,"column":1}},"children":[{"type":"text","value":"Immutable data collection methods (unable to change how data has been collected and stored)","position":{"start":{"line":53,"column":1},"end":{"line":53,"column":1}},"key":"L2ENMbwenj"}],"key":"ovE32YobJK"},{"type":"listItem","spread":true,"position":{"start":{"line":54,"column":1},"end":{"line":54,"column":1}},"children":[{"type":"text","value":"Stringent data privacy requirements","position":{"start":{"line":54,"column":1},"end":{"line":54,"column":1}},"key":"NVlnNFYTzl"}],"key":"P1PKxMfO7t"},{"type":"listItem","spread":true,"position":{"start":{"line":55,"column":1},"end":{"line":55,"column":1}},"children":[{"type":"text","value":"Difficulties in accessing datasets","position":{"start":{"line":55,"column":1},"end":{"line":55,"column":1}},"key":"FbnWeO3T2U"}],"key":"eD2AfIiYNH"},{"type":"listItem","spread":true,"position":{"start":{"line":56,"column":1},"end":{"line":56,"column":1}},"children":[{"type":"text","value":"Lack of or missing documentation on data generation, data structure and contextual information","position":{"start":{"line":56,"column":1},"end":{"line":56,"column":1}},"key":"ISeq9FlYJG"}],"key":"nSnZf4sZPE"},{"type":"listItem","spread":true,"position":{"start":{"line":57,"column":1},"end":{"line":57,"column":1}},"children":[{"type":"text","value":"Insufficient resources (human, computational, economic)","position":{"start":{"line":57,"column":1},"end":{"line":57,"column":1}},"key":"H8b7BdkMGp"}],"key":"sxstHc3zgs"},{"type":"listItem","spread":true,"position":{"start":{"line":58,"column":1},"end":{"line":59,"column":1}},"children":[{"type":"text","value":"Unclear scope of responsibilities within the project","position":{"start":{"line":58,"column":1},"end":{"line":58,"column":1}},"key":"VCVJ7xI8Va"}],"key":"ULIYaVo3R8"}],"key":"K012MsvTmo"},{"type":"paragraph","position":{"start":{"line":60,"column":1},"end":{"line":60,"column":1}},"children":[{"type":"text","value":"In an ideal situation, some of these challenges can be mitigated if communication with Data Wranglers near the start of a project is encouraged and facilitated.","position":{"start":{"line":60,"column":1},"end":{"line":60,"column":1}},"key":"itwVcfEPKL"}],"key":"dw2VeVvhBA"},{"type":"heading","depth":2,"position":{"start":{"line":63,"column":1},"end":{"line":63,"column":1}},"children":[{"type":"text","value":"Benefits of having Data Wranglers","position":{"start":{"line":63,"column":1},"end":{"line":63,"column":1}},"key":"SVV9HVsfj6"}],"label":"cl-infrastructure-data-wranglers-benefits","identifier":"cl-infrastructure-data-wranglers-benefits","html_id":"cl-infrastructure-data-wranglers-benefits","key":"aB2VwVoszP"},{"type":"list","ordered":false,"spread":false,"position":{"start":{"line":65,"column":1},"end":{"line":71,"column":1}},"children":[{"type":"listItem","spread":true,"position":{"start":{"line":65,"column":1},"end":{"line":65,"column":1}},"children":[{"type":"text","value":"Improved communication between stakeholders: Data Wranglers will mediate the ‘language barriers’ between the different people involved in collecting the data, analysing the data and interpreting the data.","position":{"start":{"line":65,"column":1},"end":{"line":65,"column":1}},"key":"w7c5TFTK4K"}],"key":"T2ex4o2ef8"},{"type":"listItem","spread":true,"position":{"start":{"line":66,"column":1},"end":{"line":66,"column":1}},"children":[{"type":"text","value":"Centrally coordinated efforts: Data Wranglers can collect common questions and discussion topics of interest and provide answers and resources.","position":{"start":{"line":66,"column":1},"end":{"line":66,"column":1}},"key":"Lzjal6SYg1"}],"key":"vf5d5i6h43"},{"type":"listItem","spread":true,"position":{"start":{"line":67,"column":1},"end":{"line":68,"column":1}},"children":[{"type":"text","value":"Data Wranglers will provide expertise in data collection, s