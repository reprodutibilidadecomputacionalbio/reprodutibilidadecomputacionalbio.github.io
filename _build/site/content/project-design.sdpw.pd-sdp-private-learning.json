oNlzid3"},{"type":"text","value":", 2019","key":"lhhJmL9D2c"}],"enumerator":"5","key":"Sqj54ayBn2"}],"key":"dbVxvX2RUO"},{"type":"text","value":". This could be particularly harmful when considering models dealing with large amounts of user-created text ","position":{"start":{"line":32,"column":1},"end":{"line":32,"column":1}},"key":"r5hV5dwP6Z"},{"type":"citeGroup","kind":"parenthetical","children":[{"type":"cite","kind":"parenthetical","label":"brownWhatDoesIt2022","identifier":"brownwhatdoesit2022","children":[{"type":"text","value":"Brown ","key":"dSrS1NmeiT"},{"type":"emphasis","children":[{"type":"text","value":"et al.","key":"lJgjkg7Gfa"}],"key":"hfqmGzkOhP"},{"type":"text","value":", 2022","key":"YkJresnlDL"}],"enumerator":"6","key":"lsIMonNTqd"}],"key":"I2EsMfRWcA"},{"type":"text","value":".","position":{"start":{"line":32,"column":1},"end":{"line":32,"column":1}},"key":"qPeum0911n"}],"key":"nS4PRDRK2D"},{"type":"heading","depth":3,"position":{"start":{"line":34,"column":1},"end":{"line":34,"column":1}},"children":[{"type":"text","value":"Federated learning","position":{"start":{"line":34,"column":1},"end":{"line":34,"column":1}},"key":"xY7UPlyUwo"}],"identifier":"federated-learning","label":"Federated learning","html_id":"federated-learning","implicit":true,"key":"vbH7Ka7I0W"},{"type":"paragraph","position":{"start":{"line":36,"column":1},"end":{"line":36,"column":1}},"children":[{"type":"text","value":"Federated Learning is a design paradigm in which the users’ data never leaves their own devices, with the model itself being broken down into a set of computations that take place on the edge, before updates are sent back to a central coordinator ","position":{"start":{"line":36,"column":1},"end":{"line":36,"column":1}},"key":"OLs7f8hivP"},{"type":"citeGroup","kind":"parenthetical","children":[{"type":"cite","kind":"parenthetical","label":"kairouzAdvancesOpenProblems2019","identifier":"kairouzadvancesopenproblems2019","children":[{"type":"text","value":"Kai