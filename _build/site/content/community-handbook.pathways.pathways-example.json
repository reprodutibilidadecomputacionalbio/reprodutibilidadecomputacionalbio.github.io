dren":[{"type":"text","value":"Community as Experts: Empowering the community to identify their own learning needs resulted in a highly relevant and targeted pathway.","position":{"start":{"line":29,"column":1},"end":{"line":29,"column":1}},"key":"v4wKC8Zupo"}],"key":"lD6lMADQIt"},{"type":"listItem","spread":true,"position":{"start":{"line":30,"column":1},"end":{"line":30,"column":1}},"children":[{"type":"text","value":"Iterative Feedback: Multiple rounds of review and feedback ensured the final content directly addressed the community’s requirements.","position":{"start":{"line":30,"column":1},"end":{"line":30,"column":1}},"key":"hvYerOE2e7"}],"key":"c1zwHlsfsZ"},{"type":"listItem","spread":true,"position":{"start":{"line":31,"column":1},"end":{"line":31,"column":1}},"children":[{"type":"text","value":"Focus on what is needed: Prioritising currently needed content significantly enhanced the pathway’s utility and prevented information overload.","position":{"start":{"line":31,"column":1},"end":{"line":31,"column":1}},"key":"O5l1bjRw0C"}],"key":"wZPSof4Zb4"},{"type":"listItem","spread":true,"position":{"start":{"line":32,"column":1},"end":{"line":33,"column":1}},"children":[{"type":"text","value":"Transparent Process: Utilising GitHub facilitated collaborative review and fostered a strong sense of ownership within the community.","position":{"start":{"line":32,"column":1},"end":{"line":32,"column":1}},"key":"Cq7x4vzfGA"}],"key":"qQEgulPIry"}],"key":"VZMrWlg14R"},{"type":"heading","depth":2,"position":{"start":{"line":35,"column":1},"end":{"line":35,"column":1}},"children":[{"type":"text","value":"Example 2: Software Citation - Thematic Workshop for Focused Content Development","position":{"start":{"line":35,"column":1},"end":{"line":35,"column":1}},"key":"h2ozfDEJZH"}],"label":"ch-pathways-example-citation","identifier":"ch-pathways-example-citation","html_id":"ch-pathways-example-citation","key":"y5KM37iFXp"},{"type":"paragraph","position":{"start":{"line":37,"colum