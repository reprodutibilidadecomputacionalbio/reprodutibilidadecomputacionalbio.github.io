{"version":1,"kind":"Article","sha256":"1ca7e95102ebe937f64c482a25c4ad992e2597297fd17f66d60cff1ae67f5aa5","slug":"project-design.data-security.sdp.biological","location":"/project-design/data-security/sdp/biological.md","dependencies":[],"frontmatter":{"title":"Biological data","content_includes_title":false,"authors":[{"nameParsed":{"literal":"The Turing Way Community","given":"The Turing Way","family":"Community"},"name":"The Turing Way Community","id":"contributors-myst-generated-uid-0"}],"license":{"content":{"id":"CC-BY-4.0","url":"https://creativecommons.org/licenses/by/4.0/","name":"Creative Commons Attribution 4.0 International","free":true,"CC":true},"code":{"id":"MIT","url":"https://opensource.org/licenses/MIT","name":"MIT License","free":true,"osi":true}},"github":"https://github.com/the-turing-way/the-turing-way","copyright":"2019-2025","numbering":{"title":{"offset":3}},"edit_url":"https://github.com/the-turing-way/the-turing-way/blob/main/book/website/project-design/data-security/sdp/biological.md","exports":[{"format":"md","filename":"biological.md","url":"/biological-e9c2487f710e385b601a367ab5216108.md"}]},"mdast":{"type":"root","children":[{"type":"block","children":[{"type":"heading","depth":2,"position":{"start":{"line":5,"column":1},"end":{"line":5,"column":1}},"children":[{"type":"text","value":"Definition of biological data","position":{"start":{"line":5,"column":1},"end":{"line":5,"column":1}},"key":"m1y0s5cPbO"}],"label":"pd-sdp-biological-def","identifier":"pd-sdp-biological-def","html_id":"pd-sdp-biological-def","key":"s09QQvQMFO"},{"type":"paragraph","position":{"start":{"line":7,"column":1},"end":{"line":7,"column":1}},"children":[{"type":"text","value":"Sensitive biological data comes from two specific areas:","position":{"start":{"line":7,"column":1},"end":{"line":7,"column":1}},"key":"zVOHx3tDhb"}],"key":"CQ5mwMs6gI"},{"type":"list","ordered":false,"spread":false,"position":{"start":{"line":8,"column":1},"end":{"line":10,"column":1}},"children":[{"type":"listItem","spread":true,"position":{"start":{"line":8,"column":1},"end":{"line":8,"column":1}},"children":[{"type":"text","value":"Nature conservation and biodiversity data","position":{"start":{"line":8,"column":1},"end":{"line":8,"column":1}},"key":"cDOcyVRyNJ"}],"key":"X6QMyvv3me"},{"type":"listItem","spread":true,"position":{"start":{"line":9,"column":1},"end":{"line":10,"column":1}},"children":[{"type":"text","value":"Biosafety (biosecurity) data","position":{"start":{"line":9,"column":1},"end":{"line":9,"column":1}},"key":"moOzDER5eN"}],"key":"QiHy3vpouV"}],"key":"ds3CYwYeVw"},{"type":"heading","depth":3,"position":{"start":{"line":11,"column":1},"end":{"line":11,"column":1}},"children":[{"type":"text","value":"Nature conservation and biodiversity data","position":{"start":{"line":11,"column":1},"end":{"line":11,"column":1}},"key":"oTir8rxxcO"}],"identifier":"nature-conservation-and-biodiversity-data","label":"Nature conservation and biodiversity data","html_id":"nature-conservation-and-biodiversity-data","implicit":true,"key":"M8uuCEYivP"},{"type":"paragraph","position":{"start":{"line":13,"column":1},"end":{"line":13,"column":1}},"children":[{"type":"text","value":"Conservation and biodiversity data could be related to endangered species, protection regulations, international environmental agreements, time and location sensitive disturbances such as during breeding periods.","position":{"start":{"line":13,"column":1},"end":{"line":13,"column":1}},"key":"jXFZUExGRI"}],"key":"jWI7R6ptv9"},{"type":"paragraph","position":{"start":{"line":15,"column":1},"end":{"line":17,"column":1}},"children":[{"type":"text","value":"These organisms often appear on a sensitive species lists such as the ","position":{"start":{"line":15,"column":1},"end":{"line":15,"column":1}},"key":"NDDc0nMS73"},{"type":"link","url":"https://docs.nbnatlas.org/sensitive-species-list/","position":{"start":{"line":15,"column":1},"end":{"line":15,"column":1}},"children":[{"type":"text","value":"National Biodiversity Network Atlas sensitive species list for the UK","position":{"start":{"line":15,"column":1},"end":{"line":15,"column":1}},"key":"efgbPLkmTI"}],"urlSource":"https://docs.nbnatlas.org/sensitive-species-list/","key":"j60FK8icoZ"},{"type":"text","value":" that has been approved by the UK’s statutory bodies and relevant experts.\nThere could also be species identified on ","position":{"start":{"line":15,"column":1},"end":{"line":15,"column":1}},"key":"wmTHCgv6z0"},{"type":"link","url":"https://jncc.gov.uk/our-work/uk-bap-priority-species/","position":{"start":{"line":15,"column":1},"end":{"line":15,"column":1}},"children":[{"type":"text","value":"Biodiversity Action Plans (BAP)","position":{"start":{"line":15,"column":1},"end":{"line":15,"column":1}},"key":"ys2Ka66BHG"}],"urlSource":"https://jncc.gov.uk/our-work/uk-bap-priority-species/","key":"dV0i6km3RE"},{"type":"text","value":" or red lists of endangered species.\nFor example the ","position":{"start":{"line":15,"column":1},"end":{"line":15,"column":1}},"key":"sw1PimLt1b"},{"type":"link","url":"https://butterfly-conservation.org/sites/default/files/red-list.pdf","position":{"start":{"line":15,"column":1},"end":{"line":15,"column":1}},"children":[{"type":"text","value":"Butterfly Red list of Great Britain","position":{"start":{"line":15,"column":1},"end":{"line":15,"column":1}},"key":"BIpyqnQxvT"}],"urlSource":"https://butterfly-conservation.org/sites/default/files/red-list.pdf","key":"Q28T7ka3a2"},{"type":"text","value":" or the International Union for Conservation of Nature’s Red List of Threatened Species ","position":{"start":{"line":15,"column":1},"end":{"line":15,"column":1}},"key":"smzfKi3fW5"},{"type":"link","url":"https://www.iucnredlist.org/","position":{"start":{"line":15,"column":1},"end":{"line":15,"column":1}},"children":[{"type":"text","value":"(IUCN Red list)","position":{"start":{"line":15,"column":1},"end":{"line":15,"column":1}},"key":"KB7lUfpL0d"}],"urlSource":"https://www.iucnredlist.org/","key":"zi2WJRa5Kk"},{"type":"text","value":".","position":{"start":{"line":15,"column":1},"end":{"line":15,"column":1}},"key":"UpOOJLxwoZ"}],"key":"BNXC0nPAxI"},{"type":"paragraph","position":{"start":{"line":19,"column":1},"end":{"line":20,"column":1}},"children":[{"type":"text","value":"Being on these lists does not necessarily mean that data about these species cannot be shared openly as some organisms may be less harmed if more information is shared about them.\nAn individual risk assessment must be done in each case.","position":{"start":{"line":19,"column":1},"end":{"line":19,"column":1}},"key":"BVWpnAfv8u"}],"key":"kD9UUXJmwm"},{"type":"paragraph","position":{"start":{"line":22,"column":1},"end":{"line":22,"column":1}},"children":[{"type":"text","value":"However, some examples of threats that could result from sharing biological data and therefore this data needing to be restricted are:","position":{"start":{"line":22,"column":1},"end":{"line":22,"column":1}},"key":"DGjN4OqNTr"}],"key":"PAGICdWoL6"},{"type":"list","ordered":false,"spread":false,"position":{"start":{"line":24,"column":1},"end":{"line":29,"column":1}},"children":[{"type":"listItem","spread":true,"position":{"start":{"line":24,"column":1},"end":{"line":25,"column":1}},"children":[{"type":"paragraph","position":{"start":{"line":24,"column":1},"end":{"line":24,"column":1}},"children":[{"type":"strong","position":{"start":{"line":24,"column":1},"end":{"line":24,"column":1}},"children":[{"type":"text","value":"Collection for the pet trade and international wildlife market","position":{"start":{"line":24,"column":1},"end":{"line":24,"column":1}},"key":"EweMBiBYsY"}],"key":"pvZCiaVe9q"},{"type":"text","value":" - for example, nesting locations of rare birds that might lead to poaching of eggs or the collection of a new species such as fish from a coral reef.","position":{"start":{"line":24,"column":1},"end":{"line":24,"column":1}},"key":"PLYUx9OEjp"}],"key":"wzEJHPqWqG"}],"key":"LO7a3hEAtM"},{"type":"listItem","spread":true,"position":{"start":{"line":26,"column":1},"end":{"line":27,"column":1}},"children":[{"type":"paragraph","position":{"start":{"line":26,"column":1},"end":{"line":26,"column":1}},"children":[{"type":"strong","position":{"start":{"line":26,"column":1},"end":{"line":26,"column":1}},"children":[{"type":"text","value":"Killing of organisms for recreational hunting, for food and for cultural, social or belief reasons","position":{"start":{"line":26,"column":1},"end":{"line":26,"column":1}},"key":"RVv7o9u8fo"}],"key":"lKPuBcTJyG"},{"type":"text","value":" - for example, killing sharks to reduce human-shark interactions or for shark-fin soup.","position":{"start":{"line":26,"column":1},"end":{"line":26,"column":1}},"key":"Cyzn2qdWAl"}],"key":"AiwvMEmCh1"}],"key":"A2Zt1X5mes"},{"type":"listItem","spread":true,"position":{"start":{"line":28,"column":1},"end":{"line":29,"column":1}},"children":[{"type":"paragraph","position":{"start":{"line":28,"column":1},"end":{"line":28,"column":1}},"children":[{"type":"strong","position":{"start":{"line":28,"column":1},"end":{"line":28,"column":1}},"children":[{"type":"text","value":"Threat of habitat destruction or disturbance by humans for recreational purposes, farming, housing or to stop conservation efforts","position":{"start":{"line":28,"column":1},"end":{"line":28,"column":1}},"key":"ePVtGh0f1y"}],"key":"awaZUqZXXD"},{"type":"text","value":" - for example, disturbance of habitats by bird watchers leading to a decline in breeding or destruction of forests for monoculture farming.","position":{"start":{"line":28,"column":1},"end":{"line":28,"column":1}},"key":"LcrCgmM0Rs"}],"key":"aP74q62A6N"}],"key":"vPil0ERjBC"}],"key":"afKBkKqPJ1"},{"type":"heading","depth":3,"position":{"start":{"line":30,"column":1},"end":{"line":30,"column":1}},"children":[{"type":"text","value":"Biosafety","position":{"start":{"line":30,"column":1},"end":{"line":30,"column":1}},"key":"HXQjrGVDYa"}],"identifier":"biosafety","label":"Biosafety","html_id":"biosafety","implicit":true,"key":"XdyhtneRyr"},{"type":"paragraph","position":{"start":{"line":32,"column":1},"end":{"line":34,"column":1}},"children":[{"type":"text","value":"Certain organisms can pose a direct threat to humans, animals, and plants such as diseases or pests.\nThis can also include newly developed organisms such as genetically-modified plants.\nThe whereabouts and the methods of development of these organisms may be considered sensitive.  At the same time, it must be considered that it may be important to share this data openly to prevent harm.","position":{"start":{"line":32,"column":1},"end":{"line":32,"column":1}},"key":"p4uGRxJMc3"}],"key":"uZDfsgelv7"},{"type":"paragraph","position":{"start":{"line":36,"column":1},"end":{"line":37,"column":1}},"children":[{"type":"text","value":"Some biosafety data may fall under the confidential data category due to a need to protect the commercial interests of these developed organisms.\nHowever, this unnecessarily limits transparency and public peer review of these datasets, which are then submitted to regulatory authorities ","position":{"start":{"line":36,"column":1},"end":{"line":36,"column":1}},"key":"YiS9bbjNq1"},{"type":"citeGroup","kind":"parenthetical","children":[{"type":"cite","kind":"parenthetical","label":"Nielsen2013biosafety","identifier":"nielsen2013biosafety","children":[{"type":"text","value":"Nielsen, 2013","key":"CTdM11CRBg"}],"enumerator":"1","key":"NTPj3I1561"}],"key":"r7XtSWxcEb"},{"type":"text","value":".","position":{"start":{"line":36,"column":1},"end":{"line":36,"column":1}},"key":"LMGMTpGQc7"}],"key":"MrlH9Xiyjv"},{"type":"heading","depth":2,"position":{"start":{"line":40,"column":1},"end":{"line":40,"column":1}},"children":[{"type":"text","value":"Benefits of opening up sensitive biological data","position":{"start":{"line":40,"column":1},"end":{"line":40,"column":1}},"key":"nPlXulsMVu"}],"label":"pd-sdp-biological-benefits","identifier":"pd-sdp-biological-benefits","html_id":"pd-sdp-biological-benefits","key":"LBeONSVzXS"},{"type":"paragraph","position":{"start":{"line":42,"column":1},"end":{"line":43,"column":1}},"children":[{"type":"text","value":"There are clear benefits to making biological data freely available, including for not-for-profit decision making, education, research and other public benefit.\nTherefore data should only be restricted when harm to the environment, harm to people, or harm to sustainability is possible.","position":{"start":{"line":42,"column":1},"end":{"line":42,"column":1}},"key":"Yyje99WptI"}],"key":"nNkZIL43ez"},{"type":"paragraph","position":{"start":{"line":45,"column":1},"end":{"line":47,"column":1}},"children":[{"type":"text","value":"Some types of biological data, particularly related to biodiversity, should not be shared openly.\nThese data should be kept partially or completely private.\nHowever, attempts should be made to provide access to researchers or individuals who can prove a need for these data such as reuse for research purposes or to peer review research.","position":{"start":{"line":45,"column":1},"end":{"line":45,"column":1}},"key":"JnCdeLd9pu"}],"key":"ce74qlgL3r"},{"type":"paragraph","position":{"start":{"line":49,"column":1},"end":{"line":49,"column":1}},"children":[{"type":"text","value":"There are also considerations around data ownership and management of these data relating to personal data identifying the collectors or researchers involved, research use of this data or organisms, and also business activities and commercial interests.","position":{"start":{"line":49,"column":1},"end":{"line":49,"column":1}},"key":"vJz88qSuDl"}],"key":"dQht2EW20v"},{"type":"paragraph","position":{"start":{"line":51,"column":1},"end":{"line":51,"column":1}},"children":[{"type":"text","value":"Each country will have specific legal restrictions for rare and endangered species, as well as biosafety regulations, that have to be taken into account for each dataset.","position":{"start":{"line":51,"column":1},"end":{"line":51,"column":1}},"key":"BZHi59RkgC"}],"key":"ZyiROmuN1Q"},{"type":"paragraph","position":{"start":{"line":54,"column":1},"end":{"line":54,"column":1}},"children":[{"type":"text","value":"The types of data that need to be considered sensitive include occurrence data surrounding the exact location of rare, endangered or commercially valuable organisms.","position":{"start":{"line":54,"column":1},"end":{"line":54,"column":1}},"key":"EK35ae43rq"}],"key":"m6VX9AwRR9"},{"type":"paragraph","position":{"start":{"line":56,"column":1},"end":{"line":56,"column":1}},"children":[{"type":"text","value":"Sensitive occurrence data that includes exact locations for organisms can be generalised in different ways to minimise or take away the potential harm of openly publishing the data.","position":{"start":{"line":56,"column":1},"end":{"line":56,"column":1}},"key":"IH6ZepInW1"}],"key":"aOKfSMOikk"},{"type":"paragraph","position":{"start":{"line":58,"column":1},"end":{"line":58,"column":1}},"children":[{"type":"text","value":"Current best practice is discussed by ","position":{"start":{"line":58,"column":1},"end":{"line":58,"column":1}},"key":"bgRs9bkkz6"},{"type":"link","url":"https://docs.gbif.org/sensitive-species-best-practices/master/en/current-best-practices-for-generalizing-sensitive-species-occurrence-data.en.pdf","position":{"start":{"line":58,"column":1},"end":{"line":58,"column":1}},"children":[{"type":"text","value":"Chapham (2020)","position":{"start":{"line":58,"column":1},"end":{"line":58,"column":1}},"key":"qvJzKhllWt"}],"urlSource":"https://docs.gbif.org/sensitive-species-best-practices/master/en/current-best-practices-for-generalizing-sensitive-species-occurrence-data.en.pdf","key":"Ex847fn5Ha"},{"type":"text","value":", who sets out 10 principles for generalising sensitive species occurrence data to enable data sharing.","position":{"start":{"line":58,"column":1},"end":{"line":58,"column":1}},"key":"vpNhBmNnzh"}],"key":"HARkFiRcGP"},{"type":"paragraph","position":{"start":{"line":60,"column":1},"end":{"line":61,"column":1}},"children":[{"type":"text","value":"It is also worth researchers conducting an assessment of the risks versus benefits of publishing biological data.","position":{"start":{"line":60,"column":1},"end":{"line":60,"column":1}},"key":"rnU8UXkUPZ"},{"type":"break","position":{"start":{"line":60,"column":1},"end":{"line":60,"column":1}},"key":"pPzBK3TAQ4"},{"type":"text","value":"The majority of the biodiversity community feel there are more benefits publishing open datasets as its future reuse could lead to greater conservation opportunities, promote community engagement and reduce duplication of survey efforts ","position":{"start":{"line":60,"column":1},"end":{"line":60,"column":1}},"key":"dpNfht0vc2"},{"type":"citeGroup","kind":"parenthetical","children":[{"type":"cite","kind":"parenthetical","label":"tulloch2018decision","identifier":"tulloch2018decision","children":[{"type":"text","value":"Tulloch ","key":"ZYkUpFZEZY"},{"type":"emphasis","children":[{"type":"text","value":"et al.","key":"aBb77e867c"}],"key":"oQdv8o1Bxq"},{"type":"text","value":", 2018","key":"aFUX2RiQuQ"}],"enumerator":"2","key":"J6rJywTdfW"}],"key":"xks45wgmuO"},{"type":"text","value":".","position":{"start":{"line":60,"column":1},"end":{"line":60,"column":1}},"key":"AMYluMuNa4"}],"key":"hxtifh3KfZ"},{"type":"heading","depth":2,"position":{"start":{"line":64,"column":1},"end":{"line":64,"column":1}},"children":[{"type":"text","value":"Examples of projects and issues with biological data","position":{"start":{"line":64,"column":1},"end":{"line":64,"column":1}},"key":"x8upHlAYFX"}],"label":"pd-sdp-biological-examples","identifier":"pd-sdp-biological-examples","html_id":"pd-sdp-biological-examples","key":"RbNEKjfLe4"},{"type":"paragraph","position":{"start":{"line":66,"column":1},"end":{"line":66,"column":1}},"children":[{"type":"cite","url":"https://conbio.onlinelibrary.wiley.c