":"caption","children":[{"type":"paragraph","position":{"start":{"line":20,"column":1},"end":{"line":21,"column":1}},"children":[{"type":"captionNumber","kind":"figure","label":"academic-industry-partnership","identifier":"academic-industry-partnership","html_id":"academic-industry-partnership","enumerator":"1","children":[{"type":"text","value":"Figure ","key":"eauFCQD5uh"},{"type":"text","value":"1","key":"wGBcBCqeJK"},{"type":"text","value":":","key":"muwuZGb3Z7"}],"template":"Figure %s:","key":"MIcMhI2N0w"},{"type":"text","value":"Academic-Industry Partnership\n","position":{"start":{"line":20,"column":1},"end":{"line":20,"column":1}},"key":"NOhZqJgw2y"},{"type":"emphasis","position":{"start":{"line":20,"column":1},"end":{"line":20,"column":1}},"children":[{"type":"text","value":"The Turing Way","position":{"start":{"line":20,"column":1},"end":{"line":20,"column":1}},"key":"T8it1po7C1"}],"key":"muUFv5B2Jg"},{"type":"text","value":" project illustration by Scriberia. Used under a CC-BY 4.0 licence. Original version on Zenodo. ","position":{"start":{"line":20,"column":1},"end":{"line":20,"column":1}},"key":"ha2sCaa1DZ"},{"type":"cite","url":"http://doi.org/10.5281/zenodo.8169292","position":{"start":{"line":20,"column":1},"end":{"line":20,"column":1}},"children":[{"type":"text","value":"Community & Scriberia (2023)","key":"Izo5pWZ5Bh"}],"kind":"narrative","label":"https://doi.org/10.5281/zenodo.8169292","identifier":"http://doi.org/10.5281/zenodo.8169292","enumerator":"1","key":"CikjxmxOAN"}],"key":"I7rb0SnQ7C"}],"key":"p0yAz7s7ji"}],"label":"academic-industry-partnership","identifier":"academic-industry-partnership","enumerator":"1","html_id":"academic-industry-partnership","key":"PRj0zp74hK"},{"type":"heading","depth":2,"position":{"start":{"line":26,"column":1},"end":{"line":26,"column":1}},"children":[{"type":"text","value":"Summary","position":{"start":{"line":26,"column":1},"end":{"line":26,"column":1}},"key":"Ez2HJgpRsT"}],"label":"cl-academic-industry-summary","identifier":"cl-academic-industry-summary","html_id":"cl-academic-industry-summary","key":"pAxH0gJqYo"},{"type":"paragraph","position":{"start":{"line":27,"column":1},"end":{"line":27,"column":1}},"children":[{"type":"text","value":"Academic-Industry collaborations can take many forms, but put simply they are when an academic institution or university collaborates with a corporation or business such as a pharmaceutical company, a consulting firm or a technology company.","position":{"start":{"line":27,"column":1},"end":{"line":27,"column":1}},"key":"xyRcQB7F8T"}],"key":"rfltQ2332c"},{"type":"paragraph","position":{"start":{"line":29,"column":1},"end":{"line":29,"column":1}},"children":[{"type":"text","value":"Whilst academia and industry are often viewed with having differing cultures, research aims and ways of working, there can be many advantages of bridging this perceived gap and working collaboratively.","position":{"start":{"line":29,"column":1},"end":{"line":29,"column":1}},"key":"ouUi4WRnVU"}],"key":"pTZLoVobaE"},{"type":"paragraph","position":{"start":{"line":31,"column":1},"end":{"line":31,"column":1}},"children":[{"type":"text","value":"Some of these advantages include:","position":{"start":{"line":31,"column":1},"end":{"line":31,"column":1}},"key":"ETNYPjcMzK"}],"key":"XaT45QMhsG"},{"type":"list","ordered":false,"spread":false,"position":{"start":{"line":32,"column":1},"end":{"line":36,"column":1}},"children":[{"type":"listItem","spread":true,"position":{"start":{"line":32,"column":1},"end":{"line":32,"column":1}},"children":[{"type":"text","value":"Access to and leveraging resources on both sides, such as funding, research expertise and skills and real-world data","position":{"start":{"line":32,"column":1},"end":{"line":32,"column":1}},"key":"MMtvs1qV0s"}],"key":"pX8XH676un"},{"type":"listItem","spread":true,"position":{"start":{"line":33,"column":1},"end":{"line":33,"column":1}},"children":[{"type":"text","value":"Combining foundational research (often the academic focus) and applied research (often the industry focus)","position":{"start":{"line":33,"column":1},"end":{"line":33,"column":1}},"key":"ypDHQSkZH8"}],"key":"qfXpBThCpj"},{"type":"listItem","spread":true,"position":{"start":{"line":34,"column":1},"end":{"line":34,"column":1}},"children":[{"type":"text","value":"Accelerating innovation and societal impact through working together on a shared research problem","position":{"start":{"line":34,"column":1},"end":{"line":34,"column":1}},"key":"vbTnE4jiQR"}],"key":"DCOJRQw5HW"},{"type":"listItem","spread":true,"position":{"start":{"line":35,"column":1},"end":{"line":36,"column":1}},"children":[{"type":"text","value":"Experience and increased career opportunities for students and researchers working as part of the collaboration","position":{"start":{"line":35,"column":1},"end":{"line":35,"column":1}},"key":"ZaXcUW3IRK"}],"key":"y44J7fKJn0"}],"key":"gTCrxibFKO"},{"type":"heading","depth":2,"position":{"start":{"line":38,"column":1},"end":{"line":38,"column":1}},"children":[{"type":"text","value":"Motivation and Background","position":{"start":{"line":38,"column":1},"end":{"line":38,"column":1}},"key":"HpIAYKHbOn"}],"label":"cl-academic-industry-motivation","identifier":"cl-academic-industry-motivation","html_id":"cl-academic-industry-motivation","key":"r1hF3ifHWq"},{"type":"paragraph","position":{"start":{"line":39,"column":1},"end":{"line":39,"column":1}},"children":[{"type":"text","value":"Academic-Industry collaborations are becoming more popular, with the ","position":{"start":{"line":39,"column":1},"end":{"line":39,"column":1}},"key":"Lnd1HYRDsn"},{"type":"link","url":"https://www.nature.com/nature-index/news-blog/the-shifting-corporate-academic-relationship-in-pictures","position":{"start":{"line":39,"column":1},"end":{"line":39,"column":1}},"children":[{"type":"text","value":"number of them more than doubling between 2012 and 2016","position":{"start":{"line":39,"column":1},"end":{"line":39,"column":1}},"key":"XCbntyG4Oj"}],"urlSource":"https://www.nature.com/nature-index/news-blog/the-shifting-corporate-academic-relationship-in-pictures","key":"QEUBe4Ku9J"},{"type":"text","value":".","position":{"start":{"line":39,"column":1},"end":{"line":39,"column":1}},"key":"rEyMZkGQhZ"}],"key":"Kd1CP6JcLF"},{"type":"paragraph","position":{"start":{"line":41,"column":1},"end":{"line":44,"column":1}},"children":[{"type":"text","value":"We wanted to create a chapter as a starting point for those looking to set up an academic-industry collaboration, however a lot of the chapter content can be applied to any collaboration.\nSimilarly, a lot of existing content in ","position":{"start":{"line":41,"column":1},"end":{"line":41,"column":1}},"key":"oRPT8NFfsr"},{"type":"emphasis","position":{"start":{"line":41,"column":1},"end":{"line":41,"column":1}},"children":[{"type":"text","value":"The Turing Way","position":{"start":{"line":41,"column":1},"end":{"line":41,"column":1}},"key":"uLf1yzZlnD"}],"key":"K2ujsCewgU"},{"type":"text","value":" also applies and we have signposted to this throughout.\nMany academic-industry collaborations are very similar in nature to academic research projects, with some small nuances that we have tried to capture.\nOverall we hope the chapter will give insights into how to set up for success in practice for a partnership between academia and industry, through navigating expectations, ways of working and bringing in elements such as community building.","position":{"start":{"line":41,"column":1},"end":{"line":41,"column":1}},"key":"qOtecFTKB7"}],"key":"GbBG2mUial"},{"type":"paragraph","position":{"start":{"line":46,"column":1},"end":{"line":47,"column":1}},"children":[{"type":"text","value":"The basis of the following subchapters are mainly from the experience of the ","position":{"start":{"line":46,"column":1},"end":{"line":46,"column":1}},"key":"Lnw53H2L5K"},{"type":"link","url":"https://www.turing.ac.uk/research/research-projects/alan-turing-institute-roche-strategic-partnership","position":{"start":{"line":46,"column":1},"end":{"line":46,"column":1}},"children":[{"type":"text","value":"Turing-Roche partnership","position":{"start":{"line":46,"column":1},"end":{"line":46,"column":1}},"key":"G6AOgtHoqA"}],"urlSource":"https://www.turing.ac.uk/research/research-projects/alan-turing-institute-roche-strategic-partnership","key":"hOxOmTK2xC"},{"type":"text","value":".\nWe would welcome any further input and content from other academic-industry collaborations.","position":{"start":{"line":46,"column":1},"end":{"line":46,"column":1}},"key":"B9464nWqoZ"}],"key":"MFILVga9Dq"},{"type":"paragraph","position":{"start":{"line":49,"column":1},"end":{"line":49,"column":1}},"children":[{"type":"text","value":"In this chapter we are using the terms ‘collaboration’ and ‘partnership’ interchangeably but understand these may confer different meanings depending on the context.","position":{"start":{"line":49,"column":1},"end":{"line":49,"column":1}},"key":"coXxN25aYJ"}],"key":"MVJgO8EVwX"}],"key":"fnMoi47HEx"}],"key":"nvCvHNcy5t"},"references":{"cite":{"order":["https://doi.org/10.5281/zenodo.8169292"],"data":{"https://doi.org/10.5281/zenodo.8169292":{"label":"https://doi.org/10.5281/zenodo.8169292","enumerator":"1","doi":"10.5281/ZENODO.8169292","html":"Community, T. T. W., & Scriberia. (2023). <i>Illustrations from The Turing Way: Shared under CC-BY 4.0 for reuse</i>. Zenodo. <a target=\"_blank\" rel=\"noreferrer\" href=\"https://doi.org/10.5281/ZENODO.8169292\">10.5281/ZENODO.8169292</a>","url":"https://doi.org/10.5281/ZENODO.8169292"}}}}}