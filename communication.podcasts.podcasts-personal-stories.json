{"version":1,"kind":"Article","sha256":"4425f74199f234620708d8f043ab5f8bbb453633832fbbb57ccdb38c97f02ec6","slug":"communication.podcasts.podcasts-personal-stories","location":"/communication/podcasts/podcasts-personal-stories.md","dependencies":[],"frontmatter":{"title":"Personal stories - Podcasts","content_includes_title":false,"authors":[{"nameParsed":{"literal":"The Turing Way Community","given":"The Turing Way","family":"Community"},"name":"The Turing Way Community","id":"contributors-myst-generated-uid-0"}],"license":{"content":{"id":"CC-BY-4.0","url":"https://creativecommons.org/licenses/by/4.0/","name":"Creative Commons Attribution 4.0 International","free":true,"CC":true},"code":{"id":"MIT","url":"https://opensource.org/licenses/MIT","name":"MIT License","free":true,"osi":true}},"github":"https://github.com/the-turing-way/the-turing-way","copyright":"2019-2025","numbering":{"title":{"offset":2}},"edit_url":"https://github.com/the-turing-way/the-turing-way/blob/main/book/website/communication/podcasts/podcasts-personal-stories.md","exports":[{"format":"md","filename":"podcasts-personal-stories.md","url":"/build/podcasts-personal-st-c4cf210262fc8db31720126280db2465.md"}]},"mdast":{"type":"root","children":[{"type":"block","children":[{"type":"heading","depth":2,"position":{"start":{"line":4,"column":1},"end":{"line":4,"column":1}},"children":[{"type":"text","value":"Zoe Ingram and Emma Harris: Orion Open Science Podcast","position":{"start":{"line":4,"column":1},"end":{"line":4,"column":1}},"key":"MRZxdudu3C"}],"identifier":"zoe-ingram-and-emma-harris-orion-open-science-podcast","label":"Zoe Ingram and Emma Harris: Orion Open Science Podcast","html_id":"zoe-ingram-and-emma-harris-orion-open-science-podcast","implicit":true,"key":"iI3ngWAubm"},{"type":"paragraph","position":{"start":{"line":6,"column":1},"end":{"line":6,"column":1}},"children":[{"type":"text","value":"The ORION Open Science podcast is structured around a series of interviews with interesting guests about a variety of topics related to Open Science.","position":{"start":{"line":6,"column":1},"end":{"line":6,"column":1}},"key":"YRg3omFv4c"}],"key":"OKDTV0CrMb"},{"type":"paragraph","position":{"start":{"line":8,"column":1},"end":{"line":8,"column":1}},"children":[{"type":"text","value":"It is also co-hosted and co-produced by ","position":{"start":{"line":8,"column":1},"end":{"line":8,"column":1}},"key":"wERbD7aHkm"},{"type":"strong","position":{"start":{"line":8,"column":1},"end":{"line":8,"column":1}},"children":[{"type":"text","value":"Dr. Luiza Bengtsson","position":{"start":{"line":8,"column":1},"end":{"line":8,"column":1}},"key":"NAk8IcvDcc"}],"key":"Bgf4P4sRzr"},{"type":"text","value":".","position":{"start":{"line":8,"column":1},"end":{"line":8,"column":1}},"key":"A4kP5ECxfy"}],"key":"XlRYc8sdyG"},{"type":"paragraph","position":{"start":{"line":10,"column":1},"end":{"line":10,"column":1}},"children":[{"type":"text","value":"If you want to find out more information about the podcast then look at their ","position":{"start":{"line":10,"column":1},"end":{"line":10,"column":1}},"key":"aCjYSlLfhd"},{"type":"link","url":"https://www.orion-openscience.eu/publications/training-materials/201902/podcasts","position":{"start":{"line":10,"column":1},"end":{"line":10,"column":1}},"children":[{"type":"text","value":"website","position":{"start":{"line":10,"column":1},"end":{"line":10,"column":1}},"key":"vHmK1LFzpv"}],"urlSource":"https://www.orion-openscience.eu/publications/training-materials/201902/podcasts","key":"gbzaMlFnQq"},{"type":"text","value":" and the ","position":{"start":{"line":10,"column":1},"end":{"line":10,"column":1}},"key":"sz6WEiBp8U"},{"type":"link","url":"https://orionopenscience.podbean.com/","position":{"start":{"line":10,"column":1},"end":{"line":10,"column":1}},"children":[{"type":"text","value":"podcast on PodBean","position":{"start":{"line":10,"column":1},"end":{"line":10,"column":1}},"key":"BgUXnEmYBo"}],"urlSource":"https://orionopenscience.podbean.com/","key":"zsvwxpeAmL"},{"type":"text","value":".","position":{"start":{"line":10,"column":1},"end":{"line":10,"column":1}},"key":"w7WG09jGYu"}],"key":"YBWPybsEWJ"},{"type":"paragraph","position":{"start":{"line":12,"column":1},"end":{"line":12,"column":1}},"children":[{"type":"text","value":"We would like to thank ","position":{"start":{"line":12,"column":1},"end":{"line":12,"column":1}},"key":"hnRTMycC9S"},{"type":"strong","position":{"start":{"line":12,"column":1},"end":{"line":12,"column":1}},"children":[{"type":"text","value":"Zoe Ingram","position":{"start":{"line":12,"column":1},"end":{"line":12,"column":1}},"key":"ftetuAOmWK"}],"key":"khe1mWMxY8"},{"type":"text","value":" and ","position":{"start":{"line":12,"column":1},"end":{"line":12,"column":1}},"key":"VAzbqegwzt"},{"type":"strong","position":{"start":{"line":12,"column":1},"end":{"line":12,"column":1}},"children":[{"type":"text","value":"Emma Harris","position":{"start":{"line":12,"column":1},"end":{"line":12,"column":1}},"key":"VixHU3aiXa"}],"key":"YiPvGUXeux"},{"type":"text","value":" for answering our interview questions about their very successful and enjoyable blog series.","position":{"start":{"line":12,"column":1},"end":{"line":12,"column":1}},"key":"Ug4pcD3cOx"}],"key":"ooedpLZcxB"},{"type":"heading","depth":2,"position":{"start":{"line":14,"column":1},"end":{"line":14,"column":1}},"children":[{"type":"text","value":"1. Why did you start your podcast?","position":{"start":{"line":14,"column":1},"end":{"line":14,"column":1}},"key":"G9kkmnTb3Q"}],"identifier":"id-1-why-did-you-start-your-podcast","label":"1. Why did you start your podcast?","html_id":"id-1-why-did-you-start-your-podcast","implicit":true,"key":"z8uaoYQela"},{"type":"paragraph","position":{"start":{"line":16,"column":1},"end":{"line":16,"column":1}},"children":[{"type":"strong","position":{"start":{"line":16,"column":1},"end":{"line":16,"column":1}},"children":[{"type":"text","value":"Emma","position":{"start":{"line":16,"column":1},"end":{"line":16,"column":1}},"key":"smiChpvhqg"}],"key":"b0UHmCsbCr"},{"type":"text","value":" - While there are a good number of podcasts on Open Science now (which is great), back in 2018 there was only Open Science Radio, which was excellent but often only in German.","position":{"start":{"line":16,"column":1},"end":{"line":16,"column":1}},"key":"RwQn5Jtr64"}],"key":"yk2833lRfo"},{"type":"paragraph","position":{"start":{"line":18,"column":1},"end":{"line":18,"column":1}},"children":[{"type":"strong","position":{"start":{"line":18,"column":1},"end":{"line":18,"column":1}},"children":[{"type":"text","value":"Zoe","position":{"start":{"line":18,"column":1},"end":{"line":18,"column":1}},"key":"HFs53OnEuk"}],"key":"CxZ8CoXRdU"},{"type":"text","value":" - We are part of the Orion Open Science Project and our task is doing training so Luiza decided that doing the podcast as part of the training material would be a good idea.","position":{"start":{"line":18,"column":1},"end":{"line":18,"column":1}},"key":"q8kp8hMROf"}],"key":"IFPT5BsyW1"},{"type":"paragraph","position":{"start":{"line":20,"column":1},"end":{"line":21,"column":1}},"children":[{"type":"strong","position":{"start":{"line":20,"column":1},"end":{"line":20,"column":1}},"children":[{"type":"text","value":"Emma","position":{"start":{"line":20,"column":1},"end":{"line":20,"column":1}},"key":"fO4ayyaPPx"}],"key":"X7X5Y9zyOw"},{"type":"text","value":" - We felt that a podcast, with the format of interviews with experts on different topics, would be an easy and entertaining way of communicating open science concepts to researchers and others.\nEven with the rise of e-learning and online workshops, there is an upper limit on how many people you can reach with traditional training. The podcast allows everyone to have access, and for different aspects of open science to be explored in-depth.","position":{"start":{"line":20,"column":1},"end":{"line":20,"column":1}},"key":"v8PrPtKhIf"}],"key":"VntItJPKSq"},{"type":"heading","depth":2,"position":{"start":{"line":23,"column":1},"end":{"line":23,"column":1}},"children":[{"type":"text","value":"2. How did you get started?","position":{"start":{"line":23,"column":1},"end":{"line":23,"column":1}},"key":"Vt8MHGTVHt"}],"identifier":"id-2-how-did-you-get-started","label":"2. How did you get started?","html_id":"id-2-how-did-you-get-started","implicit":true,"key":"LftLGKn1Ir"},{"type":"paragraph","position":{"start":{"line":25,"column":1},"end":{"line":26,"column":1}},"children":[{"type":"strong","position":{"start":{"line":25,"column":1},"end":{"line":25,"column":1}},"children":[{"type":"text","value":"Zoe","position":{"start":{"line":25,"column":1},"end":{"line":25,"column":1}},"key":"r5JoZ0cjnr"}],"key":"qOor2r3XBE"},{"type":"text","value":" - It was something that none of us had done before.\nWe had a colleague whose wife had skills in this area so she taught us how to get started. She gave Emma and Luiza feedback about speaking and how to adjust their voices for speaking on the podcast.","position":{"start":{"line":25,"column":1},"end":{"line":25,"column":1}},"key":"LhUUdxgpWY"}],"key":"K4mC6JwpAG"},{"type":"paragraph","position":{"start":{"line":28,"column":1},"end":{"line":28,"column":1}},"children":[{"type":"strong","position":{"start":{"line":28,"column":1},"end":{"line":28,"column":1}},"children":[{"type":"text","value":"Emma","position":{"start":{"line":28,"column":1},"end":{"line":28,"column":1}},"key":"oTpwhigKz8"}],"key":"eauUJE0S83"},{"type":"text","value":" - We just started interviewing people and learned by doing, so the early podcasts are lower in quality and we are pretty awkward! But slowly we learned to be more relaxed and how to handle the technical side of things more competently.","position":{"start":{"line":28,"column":1},"end":{"line":28,"column":1}},"key":"nSCqpY2EAn"}],"key":"rj2VtW1uZ7"},{"type":"paragraph","position":{"start":{"line":30,"column":1},"end":{"line":30,"column":1}},"children":[{"type":"strong","position":{"start":{"line":30,"column":1},"end":{"line":30,"column":1}},"children":[{"type":"text","value":"Zoe","position":{"start":{"line":30,"column":1},"end":{"line":30,"column":1}},"key":"nINF9jSPsJ"}],"key":"Gy6ERvyrMH"},{"type":"text","value":" - We did get a sound editor to help with it. This was for the technical and time constraint in doing this.","position":{"start":{"line":30,"column":1},"end":{"line":30,"column":1}},"key":"VnfWIdR418"}],"key":"iVyVcvaJvl"},{"type":"paragraph","position":{"start":{"line":32,"column":1},"end":{"line":32,"column":1}},"children":[{"type":"text","value":"We made a jingle and there is a new jingle for the new season.","position":{"start":{"line":32,"column":1},"end":{"line":32,"column":1}},"key":"sChX02JS6R"}],"key":"sPbJ17EUkS"},{"type":"heading","depth":2,"position":{"start":{"line":34,"column":1},"end":{"line":34,"column":1}},"children":[{"type":"text","value":"3. What impact do you think your podcast is making on your research/project/community?","position":{"start":{"line":34,"column":1},"end":{"line":34,"column":1}},"key":"zA9Q0syYS3"}],"identifier":"id-3-what-impact-do-you-think-your-podcast-is-making-on-your-research-project-community","label":"3. What impact do you think your podcast is making on your research/project/community?","html_id":"id-3-what-impact-do-you-think-your-podcast-is-making-on-your-research-project-community","implicit":true,"key":"H3gGLaC258"},{"type":"paragraph","position":{"start":{"line":36,"column":1},"end":{"line":37,"column":1}},"children":[{"type":"strong","position":{"start":{"line":36,"column":1},"end":{"line":36,"column":1}},"children":[{"type":"text","value":"Zoe","position":{"start":{"line":36,"column":1},"end":{"line":36,"column":1}},"key":"Y5uoFY6Mbc"}],"key":"VoXPZEW1fI"},{"type":"text","value":" - It is hard to know who is listening as you are sending things out to a black box.\nWe do get mail and feedback about the podcast from people that want to suggest themes and volunteer take part.","position":{"start":{"line":36,"column":1},"end":{"line":36,"column":1}},"key":"eBHHI4H87K"}],"key":"F8SoRxlsP9"},{"type":"paragraph","position":{"start":{"line":39,"column":1},"end":{"line":40,"column":1}},"children":[{"type":"strong","position":{"start":{"line":39,"column":1},"end":{"line":39,"column":1}},"children":[{"type":"text","value":"Emma","position":{"start":{"line":39,"column":1},"end":{"line":39,"column":1}},"key":"vl9uMq2ByU"}],"key":"VlGwj6Mv73"},{"type":"text","value":" - The global impact always makes me happy - we have people from all over the world on the show and have heard from listeners from even more countries.\nI think the podcast does not have a huge audience in numbers but it is very geographically diverse.","position":{"start":{"line":39,"column":1},"end":{"line":39,"column":1}},"key":"DSth9nMJgZ"}],"key":"TupsjpKJR8"},{"type":"paragraph","position":{"start":{"line":42,"column":1},"end":{"line":44,"column":1}},"children":[{"type":"text","value":"The episodes are very useful and engaging on particular topics.\nFor example, I recently gave a talk on Creative Commons Licenses and someone wrote to me who had missed it and wanted some information, so I referred them to the podcast episode we did with Cable Green, the CEO of Creative Commons.\nSo they are an excellent open educational resource, as well as being generally interesting.","position":{"start":{"line":42,"column":1},"end":{"line":42,"column":1}},"key":"SONoTC0stZ"}],"key":"qdRvY9vK4k"},{"type":"paragraph","position":{"start":{"line":46,"column":1},"end":{"line":47,"column":1}},"children":[{"type":"strong","position":{"start":{"line":46,"column":1},"end":{"line":46,"column":1}},"children":[{"type":"text","value":"Zoe","position":{"start":{"line":46,"column":1},"end":{"line":46,"column":1}},"key":"XvoarwPVQi"}],"key":"ED7zfihryk"},{"type":"text","value":" - I like talking to people and have missed our Institute’s Beer Hour in which I got to meet and speak to scientists about their work.\nI enjoy having these types of conversations about open science.","position":{"start":{"line":46,"column":1},"end":{"line":46,"column":1}},"key":"kzJpVJF0yd"}],"key":"cIR1x0opK6"},{"type":"paragraph","position":{"start":{"line":49,"column":1},"end":{"line":50,"column":1}},"children":[{"type":"text","value":"I think the podcast is a format that is flexible and it is easier to follow a conversation than say listening to someone give a talk.\nThis means that someone can listen to the podcast while they are doing another activity as it is a different way to take in information.","position":{"start":{"line":49,"column":1},"end":{"line":49,"column":1}},"key":"xtCOa6LQtL"}],"key":"N49DmtFysx"},{"type":"paragraph","position":{"start":{"line":52,"column":1},"end":{"line":55,"column":1}},"children":[{"type":"text","value":"Luiza pointed out what a benefit it is for us to be able to learn so much from the guests.\nIt is hard to stay on top of so many fast-paced developments.\nSo, we are in that sense lucky to be able to get to the level of depth of information through the conversations and ask the questions that we want to be answered.\nI also think it connects people as they hear about other researchers work.","position":{"start":{"line":52,"column":1},"end":{"line":52,"column":1}},"key":"vx3m7qoD6E"}],"key":"YfYX4hFU64"},{"type":"heading","depth":2,"position":{"start":{"line":57,"column":1},"end":{"line":57,"column":1}},"children":[{"type":"text","value":"4. What tools/software do you use most in your podcast?","position":{"start":{"line":57,"column":1},"end":{"line":57,"column":1}},"key":"FhspiqOPQ1"}],"identifier":"id-4-what-tools-software-do-you-use-most-in-your-podcast","label":"4. What tools/software do you use most in your podcast?","html_id":"id-4-what-tools-software-do-you-use-most-in-your-podcast","implicit":true,"key":"X3IyBcVC18"},{"type":"paragraph","position":{"start":{"line":59,"column":1},"end":{"line":61,"column":1}},"children":[{"type":"strong","position":{"start":{"line":59,"column":1},"end":{"line":59,"column":1}},"children":[{"type":"text","value":"Emma","position":{"start":{"line":59,"column":1},"end":{"line":59,"column":1}},"key":"ZbNokKIIFJ"}],"key":"fsaDHb31em"},{"type":"text","value":" - The mic we used pre-pandemic was an H6 Zoom.\nFor online interviews: Zencastr and a decent headset with a built-in mic.\nEditing was done on Adobe Premiere Pro.","position":{"start":{"line":59,"column":1},"end":{"line":59,"column":1}},"key":"s39O9qvUUF"}],"key":"miKZ0pcfQZ"},{"type":"paragraph","position":{"start":{"line":63,"column":1},"end":{"line":64,"column":1}},"children":[{"type":"strong","position":{"start":{"line":63,"column":1},"end":{"line":63,"column":1}},"children":[{"type":"text","value":"Zoe","position":{"start":{"line":63,"column":1},"end":{"line":63,"column":1}},"key":"tPQm5Z0psE"}],"key":"MahbbhUK0z"},{"type":"text","value":" - The podcast is hosted on PodBean.\nWe are thinking of going over to Audacity for editing as it is free, open source software.","position":{"start":{"line":63,"column":1},"end":{"line":63,"column":1}},"key":"Z3OH6jfvN7"}],"key":"F75zzZZgX1"},{"type":"heading","depth":2,"position":{"start":{"line":66,"column":1},"end":{"line":66,"column":1}},"children":[{"type":"text","value":"5. Do you have any top tips for other people that might be interested in starting a podcast?","position":{"start":{"line":66,"column":1},"end":{"line":66,"column":1}},"key":"DGM9KPwUhd"}],"identifier":"id-5-do-you-have-any-top-tips-for-other-people-that-might-be-interested-in-starting-a-podcast","label":"5. Do you have any top tips for other people that might be interested in starting a podcast?","html_id":"id-5-do-you-have-any-top-tips-for-other-people-that-might-be-interested-in-starting-a-podcast","implicit":true,"key":"YCmusBrHcK"},{"type":"paragraph","position":{"start":{"line":68,"column":1},"end":{"line":69,"column":1}},"children":[{"type":"strong","position":{"start":{"line":68,"column":1},"end":{"line":68,"column":1}},"children":[{"type":"text","value":"Emma","position":{"start":{"line":68,"column":1},"end":{"line":68,"column":1}},"key":"M7DRc2GJU7"}],"key":"ABuIuE9OBu"},{"type":"text","value":" - Decide on your audience and cater to them, what are their concerns and interests?\nCreate material that reflects those things.","position":{"start":{"line":68,"column":1},"end":{"line":68,"column":1}},"key":"DJTIXsG227"}],"key":"Yl7LPJUa5h"},{"type":"paragraph","position":{"start":{"line":71,"column":1},"end":{"line":73,"column":1}},"children":[{"type":"text","value":"It is incredibly hard to relax ‘on tape’, even if you are a good public speaker.\nIt is normal to feel anxious so don’t be surprised if you get very tense.\nThe best thing to do to overcome this is to press record and then just chat with people, even do some warm-up/ice breaker type exercises to get everyone comfortable.","position":{"start":{"line":71,"column":1},"end":{"line":71,"column":1}},"key":"hLhAuVXGmX"}],"key":"uptICVYSfm"},{"type":"paragraph","position":{"start":{"line":75,"column":1},"end":{"line":76,"column":1}},"children":[{"type":"text","value":"You will hate your voice, everybody does, but you will learn to accept it in time.\nAim to speak normally, as you would to someone sitting next to you. Smile when you speak, it makes a difference.","position":{"start":{"line":75,"column":1},"end":{"line":75,"column":1}},"key":"jSeRiHZ6mf"}],"key":"Tm7OV7uWES"},{"type":"paragraph","position":{"start":{"line":78,"column":1},"end":{"line":78,"column":1}},"children":[{"type":"text","value":"Always ensure you get people to say their name at least once on the recording so you have the correct pronunciation (and check people’s preferred pronouns too).","position":{"start":{"line":78,"column":1},"end":{"line":78,"column":1}},"key":"RucSTPFWHT"}],"key":"TbXAyqzXV0"},{"type":"paragraph","position":{"start":{"line":80,"column":1},"end":{"line":80,"column":1}},"children":[{"type":"text","value":"Take some brief notes as they speak so you can come back to earlier points or ask follow-up questions.","position":{"start":{"line":80,"column":1},"end":{"line":80,"column":1}},"key":"yWILWqus0V"}],"key":"JmWcCs7X4g"},{"type":"paragraph","position":{"start":{"line":82,"column":1},"end":{"line":83,"column":1}},"children":[{"type":"text","value":"Aim for quality, not quantity, better to have one great episode a month than four okay ones.\nHowever, if you are looking to have a large audience it is probably best to be weekly or biweekly.","position":{"start":{"line":82,"column":1},"end":{"line":82,"column":1}},"key":"M4EsQTzRM2"}],"key":"iWRoO3o4hh"},{"type":"paragraph","position":{"start":{"line":85,"column":1},"end":{"line":86,"column":1}},"children":[{"type":"strong","position":{"start":{"line":85,"column":1},"end":{"line":85,"column":1}},"children":[{"type":"text","value":"Zoe","position":{"start":{"line":85,"column":1},"end":{"line":85,"column":1}},"key":"ZehGFU9Ocm"}],"key":"yDFZlVNV0E"},{"type":"text","value":" - Do it as a conversation and have an idea of the audience in mind.\nMake sure that you ask questions to get the information for listeners to put ideas into practice - empower people to take action.","position":{"start":{"line":85,"column":1},"end":{"line":85,"column":1}},"key":"pYC7LOhtmZ"}],"key":"IXUoJe8xSi"},{"type":"paragraph","position":{"start":{"line":88,"column":1},"end":{"line":88,"column":1}},"children":[{"type":"text","value":"If you are trying to start a listener base, then try to interview people that have a lot of followers on Twitter so they can retweet.","position":{"start":{"line":88,"column":1},"end":{"line":88,"column":1}},"key":"SSplCZPjfZ"}],"key":"roxwOqgJij"},{"type":"paragraph","position":{"start":{"line":90,"column":1},"end":{"line":90,"column":1}},"children":[{"type":"text","value":"Also, make sure you block the whole day for the interview as it is time-consuming and takes up a lot of energy.","position":{"start":{"line":90,"column":1},"end":{"line":90,"column":1}},"key":"SRulnlboWh"}],"key":"fjEWhAqc2x"},{"type":"heading","depth":2,"position":{"start":{"line":92,"column":1},"end":{"line":92,"column":1}},"children":[{"type":"text","value":"6. Do you have any tips on things to avoid?","position":{"start":{"line":92,"column":1},"end":{"line":92,"column":1}},"key":"c3lTBLB8u3"}],"identifier":"id-6-do-you-have-any-tips-on-things-to-avoid","label":"6. Do you have any tips on things to avoid?","html_id":"id-6-do-you-have-any-tips-on-things-to-avoid","implicit":true,"key":"q62jLkiXSM"},{"type":"paragraph","position":{"start":{"line":94,"column":1},"end":{"line":94,"column":1}},"children":[{"type":"strong","position":{"start":{"line":94,"column":1},"end":{"line":94,"column":1}},"children":[{"type":"text","value":"Emma","position":{"start":{"line":94,"column":1},"end":{"line":94,"column":1}},"key":"YwDxLwDxDF"}],"key":"ls9FAn6ADB"},{"type":"text","value":" - See other podcasts as opportunities for collaboration and cross-promotion, not competition.","position":{"start":{"line":94,"column":1},"end":{"line":94,"column":1}},"key":"D8NiI5WxGj"}],"key":"FFcQ0pHnGw"},{"type":"paragraph","position":{"start":{"line":96,"column":1},"end":{"line":96,"column":1}},"children":[{"type":"text","value":"Don’t be vain: if you are interviewing you should try and ensure your guests sound good and edit your content down unless you are making a truly important point.","position":{"start":{"line":96,"column":1},"end":{"line":96,"column":1}},"key":"LMdr1j33zi"}],"key":"zFq6H4cO5Z"},{"type":"paragraph","position":{"start":{"line":98,"column":1},"end":{"line":98,"column":1}},"children":[{"type":"text","value":"Don’t be afraid to ask ‘stupid questions’: if you don’t understand something your listeners might not either.","position":{"start":{"line":98,"column":1},"end":{"line":98,"column":1}},"key":"xTAnIrETyt"}],"key":"BNgdw5fCIu"},{"type":"paragraph","position":{"start":{"line":100,"column":1},"end":{"line":100,"column"